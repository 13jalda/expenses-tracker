{
  "name": "expense-tracker-frontend",
  "version": "1.0.0",
  "description": "Frontend for the Expense Tracker application using React, Axios, Webpack, and Babel",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack serve --mode development --open",
    "build": "webpack --mode production",
    "lint": "eslint src/**"
  },
  "dependencies": {
    "axios": "^1.5.0",                       
    "react": "^18.2.0",                       
    "react-dom": "^18.2.0",                   
    "react-router-dom": "^6.4.0",             
    "chart.js": "^4.2.0",                     
    "react-chartjs-2": "^5.2.0",              
    "styled-components": "^6.0.0",            
    "sass": "^1.60.0",                        
    "dotenv": "^16.0.0"                       
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",                 
    "@babel/preset-env": "^7.22.0",           
    "@babel/preset-react": "^7.18.0",         
    "babel-loader": "^9.1.0",                 
    "webpack": "^5.88.0",                     
    "webpack-cli": "^5.1.0",                  
    "webpack-dev-server": "^4.15.0",          
    "html-webpack-plugin": "^5.5.0",          
    "css-loader": "^6.8.0",                   
    "style-loader": "^3.3.0",                 
    "sass-loader": "^13.0.0",                 
    "file-loader": "^6.2.0",                  
    "eslint": "^8.30.0",                      
    "eslint-plugin-react": "^7.31.0",         
    "eslint-webpack-plugin": "^4.3.0"         
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "> 0.2%"
  ],
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es2021": true
    }
  }
}
